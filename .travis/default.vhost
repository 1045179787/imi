server {
    listen 127.0.0.1:13010;

    server_name http;

    location / {
      proxy_set_header Host $host;
      proxy_pass http://127.0.0.1:13000;
    }
}

server {
    listen 127.0.0.1:13015;

    server_name http_session_test;

    location / {
      proxy_set_header Host $host;
      proxy_pass http://127.0.0.1:13005;
    }
}
